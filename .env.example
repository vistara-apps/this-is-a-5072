# KnowYourRights Cards - Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_BASE_URL=https://api.openai.com/v1

# Stripe Payment Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Pinata IPFS Configuration
REACT_APP_PINATA_API_KEY=your_pinata_api_key_here
REACT_APP_PINATA_SECRET_KEY=your_pinata_secret_key_here

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api

# App Configuration
REACT_APP_APP_NAME=KnowYourRights Cards
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CRASH_REPORTING=false
REACT_APP_ENABLE_PREMIUM_FEATURES=true

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
